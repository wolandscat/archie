definition
	OBSERVATION[at0000.1] matches {	-- Paediatric Glasgow Coma Scale
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Best eye response (E)
										value matches {
											1|[local::at0010], 	-- No response
											2|[local::at0011], 	-- Eyes open in response to pain
											3|[local::at0012], 	-- Eyes open in response to speech
											4|[local::at0013]  	-- Spontaneous eye opening
										}
									}
									ELEMENT[at0034] occurrences matches {0..1} matches {	-- Not able to test eye response
										value matches {
											DV_BOOLEAN matches {
												value matches {True}
											}
										}
									}
									ELEMENT[at0038] occurrences matches {0..1} matches {	-- Reason for inability to test eye response
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0008.1] occurrences matches {0..1} matches {	-- Best motor response - child (M)
										value matches {
											1|[local::at0019], 	-- No response
											2|[local::at0020], 	-- Extensor response
											3|[local::at0021], 	-- Flexor response
											4|[local::at0022], 	-- Withdrawal response
											5|[local::at0023], 	-- Localises pain
											6|[local::at0024]  	-- Obeys commands
										}
									}
									ELEMENT[at0.41] occurrences matches {0..1} matches {	-- Best motor response - infant (M)
										value matches {
											1|[local::at0.42], 	-- No response
											2|[local::at0.43], 	-- Extensor response
											3|[local::at0.44], 	-- Flexor response
											4|[local::at0.45], 	-- Withdrawal response
											5|[local::at0.46], 	-- Withdraws to touch
											6|[local::at0.47]  	-- Moves spontaneously
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {	-- Not able to test motor response
										value matches {
											DV_BOOLEAN matches {
												value matches {True}
											}
										}
									}
									ELEMENT[at0040] occurrences matches {0..1} matches {	-- Reason for inability to test motor response
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0007.1] occurrences matches {0..1} matches {	-- Best verbal response - adult (V)
										value matches {
											1|[local::at0014], 	-- No response
											2|[local::at0015], 	-- Incomprehensible sounds
											3|[local::at0016], 	-- Inappropriate speech
											4|[local::at0017], 	-- Confused conversation
											5|[local::at0018]  	-- Oriented
										}
									}
									ELEMENT[at0.48] occurrences matches {0..1} matches {	-- Best verbal response - child (V)
										value matches {
											1|[local::at0.50], 	-- No response
											2|[local::at0.51], 	-- Incomprehensible words or nonspecific sounds
											3|[local::at0.52], 	-- Inappropriate words
											4|[local::at0.53], 	-- Confused
											5|[local::at0.54]  	-- Oriented, appropriate
										}
									}
									ELEMENT[at0.49] occurrences matches {0..1} matches {	-- Best verbal response - infant (V)
										value matches {
											1|[local::at0.55], 	-- No response
											2|[local::at0.56], 	-- Moans
											3|[local::at0.57], 	-- Cries
											4|[local::at0.58], 	-- Irritable cries
											5|[local::at0.59]  	-- Coos and babbles
										}
									}
									ELEMENT[at0.60] occurrences matches {0..1} matches {	-- Best grimace response (G)
										value matches {
											1|[local::at0.61], 	-- No response
											2|[local::at0.62], 	-- Mild grimace
											3|[local::at0.63], 	-- Vigorous grimace
											4|[local::at0.64], 	-- Less than usual spontaneous activity
											5|[local::at0.65]  	-- Normal spontaneous facial/oro-motor activity
										}
									}
									ELEMENT[at0035] occurrences matches {0..1} matches {	-- Not able to test verbal response
										value matches {
											DV_BOOLEAN matches {
												value matches {True}
											}
										}
									}
									ELEMENT[at0039] occurrences matches {0..1} matches {	-- Reason for inability to test verbal response
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0026] occurrences matches {0..1} matches {	-- Total score
										value matches {
											DV_COUNT matches {
												magnitude matches {|3..15|}
											}
										}
									}
									ELEMENT[at0030] occurrences matches {0..1} matches {	-- EMV profile
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0037] occurrences matches {0..*} matches {	-- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}


